{
  "name": "jedlik-vite-quasar-minimal",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 8080 --open",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "test": "vitest --run --coverage --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "cy:open": "npx cypress open --browser=edge",
    "cy:run": "npx cypress run"
  },
  "simple-git-hooks": {
    "pre-commit": "npm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.2",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^10.0.2",
    "@vueuse/math": "^9.13.0",
    "@vueuse/shared": "^9.13.0",
    "axios": "^1.3.5",
    "pinia": "^2.0.34",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.11.10",
    "tslib": "^2.5.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue3-google-login": "^2.0.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@pinia/testing": "^0.0.16",
    "@quasar/vite-plugin": "^1.3.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "@vue/test-utils": "^2.3.2",
    "cypress": "^12.9.0",
    "eslint": "^8.38.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-vue": "^9.10.0",
    "happy-dom": "^9.3.2",
    "lint-staged": "^13.2.1",
    "sass": "1.32.12",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0"
  }
}
